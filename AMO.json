{
  "version": "1.3.1",
  "createdAt": "2017-03-02",
  "rules": [
    {
      "description": "AMO强制简体中文（无效）",
      "origin": "^https://addons.mozilla.org/\\S+/firefox/",
      "exclude": "^https://addons.mozilla.org/zh-CN/firefox/",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://addons.mozilla.org/zh-CN/firefox/",
      "example": "https://addons.mozilla.org/zh-tw/firefox/",
      "enable": false
    },
    {
      "description": "去除AMO后缀（无效）",
      "origin": "^https://addons.mozilla.org/zh-CN/firefox/addon/(\\S+)/\\?src=.*",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://addons.mozilla.org/zh-CN/firefox/addon/$1",
      "example": "https://addons.mozilla.org/zh-CN/firefox/addon/omnisidebar/?src=hp-dl-featured",
      "enable": false
    }
  ]
}
