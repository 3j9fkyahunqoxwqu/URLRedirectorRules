{
  "version": "1.3.11.20180531",
  "createdAt": "2017-03-02",
  "rules": [
    {
      "description": "直接登录：豆瓣跳过注册页面",
      "origin": "^https?://www.douban.com/accounts/register$",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://www.douban.com/accounts/login",
      "example": "http://www.douban.com/accounts/register",
      "enable": true
    },
    {
      "description": "直接登录：用户样式库直接选择用户名密码登录模式",
      "origin": "https://userstyles.org/login$",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://userstyles.org/login?view=password",
      "example": "https://userstyles.org/login",
      "enable": true
    },
    {
      "description": "直接登录：知乎",
      "origin": "^https?://www.zhihu.com/#signup$",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://www.zhihu.com/#signin",
      "example": "https://www.zhihu.com/#signup",
      "enable": true
    },
    {
      "description": "直接下载：Dropbpx 跳过下载页直接进入直链",
      "origin": "^https://www.dropbox.com/s/(.*)\\?dl=0",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://dl.dropboxusercontent.com/s/$1",
      "example": "https://www.dropbox.com/s/rnx6mxpe5s44pa2/Redirector.txt?dl=0",
      "enable": true
    },
    {
      "description": "去除跳转：百度",
      "origin": "^http://www.spmke.com/source/bd.php\\?(https?://.+)",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "$1",
      "example": "http://www.spmke.com/source/bd.php?http://www.baidu.com",
      "enable": true
    },
    {
      "description": "去除跳转：知乎",
      "origin": "^https://www.zhihu.com/question/(https?://\\S+)",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "$1",
      "example": "https://www.zhihu.com/question/http://www.changlai.net",
      "enable": true
    },
    {
      "description": "去除尾巴：今日头条",
      "origin": "^https?://(www|m).toutiao(cdn)?.com/(a|i)(\\d+)/\\?.*",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://$1.toutiao.com/$2$3/",
      "example": "https://www.toutiao.com/i6385720374801203714/?tt_from=mobile_qq&utm_campaign=client_share&app=news_article&utm_source=mobile_qq&utm_medium=toutiao_ios",
      "enable": true
    },
    {
      "description": "去除尾巴：Twitter 短链判断失败时混入的尾部中文等字符",
      "origin": "^https?://t.co/(\\w{10}).+",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://t.co/$1",
      "example": "http://t.co/shSgWRTz51张佳玮写字的地方",
      "enable": true
    },
    {
      "description": "去除尾巴：优酷",
      "origin": "^https?://v.youku.com/v_show/id_(\\w+)(==_?.*)?.html(\\?.*)",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://v.youku.com/v_show/id_$1.html",
      "example": "https://v.youku.com/v_show/id_XMTY0MjIyMDU3Mg==.html?from=y1.2-1-105.3.5-2.1-1-1-4-0",
      "enable": true
    }
  ]
}
