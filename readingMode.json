{
  "version": "1.3.11.20180602",
  "createdAt": "2018-02-25",
  "rules": [
    {
        "description": "阅读模式：网易新闻APP分享产生地址重定向至更适合网页阅读版本",
        "origin": "c.m.163.com/news/a/(\\w+).html(\\?.*)?",
        "exclude": "",
        "methods": [],
        "types": [
          "main_frame"
        ],
        "target": "3g.163.com/news/article/$1.html",
        "example": "",
        "enable": true
    },
    {
      "description": "阅读模式：Mozest 广场无图版转至完整版",
      "origin": "^https?://g.mozest.com/archiver/tid-(\\d+).html$",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "http://g.mozest.com/viewthread.php?tid=$1",
      "example": "http://g.mozest.com/archiver/tid-38916.html",
      "enable": true
    },
    {
        "description": "阅读模式：新浪博客文章自动跳转到移动版",
        "origin": "blog.sina.com.cn/(s/blog_\\w+.html).*",
        "exclude": "",
        "methods": [],
        "types": [
          "main_frame"
        ],
        "target": "blog.sina.cn/dpool/blog/$1",
        "example": "http://blog.sina.com.cn/s/blog_4e51dd500102xdzi.html",
        "enable": true
    },
    {
      "description": "阅读模式：跳过微博看看框架",
      "origin": "weibo.wbdacdn.com/url/t/(\\w{7})/?",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "t.cn/$1",
      "example": "http://weibo.wbdacdn.com/url/t/R5mLykn",
      "enable": true
    },
    {
      "description": "阅读模式：将特定微博信息从微博看看跳转至微博HTML5版",
      "origin": "^https?://weibo.wbdacdn.com/user/\\d+/status(\\d+).html.*",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://m.weibo.cn/status/$1",
      "example": "http://weibo.wbdacdn.com/user/2803301701/status4074010167896825.html#utm_source=rss",
      "enable": true
    },
    {
      "description": "阅读模式：微博文章自动跳转到移动版",
      "origin": "^https?://(www.)?weibo.com/ttarticle/p/show",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://media.weibo.cn/article",
      "example": "http://weibo.com/ttarticle/p/show?id=2309404056722711967901",
      "enable": true
    },
    {
      "description": "自动全文：FT中文网",
      "origin": "www.ftchinese.com/story/(\\d+)$",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "www.ftchinese.com/story/$1?full=y",
      "example": "http://www.ftchinese.com/story/001053184",
      "enable": true
    },
    {
      "description": "自动全文：观察者网",
      "origin": "^https?://www.guancha.cn/(\\S+/\\d{4}_\\d{2}_\\d{2}_\\d+)(_[1-9])?.shtml$",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://www.guancha.cn/$1_s.shtml",
      "example": "http://www.guancha.cn/chenjing/2017_03_16_399011_1.shtml",
      "enable": true
    },
    {
      "description": "自动全文：法律图书馆",
      "origin": "www.law-lib.com/law/law_view.asp\\?id=(\\d+)$",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "www.law-lib.com/law/law_view1.asp?id=$1",
      "example": "http://www.law-lib.com/law/law_view.asp?id=499805",
      "enable": true
    },
    {
      "description": "原图显示：酷安",
      "origin": "^http://image.coolapk.com/(feed|apk_image)/(\\S+).(jpg|jpeg|gif|png|bmp|webp).(m|s|t).(jpg|jpeg|gif|png|bmp|webp)",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "http://image.coolapk.com/$1/$2.$3",
      "example": "http://image.coolapk.com/feed/2017/0225/689514_1487997452_9028.png.m.jpg",
      "enable": true
    },
    {
      "description": "原图显示：Lofter；可作用于InoReader",
      "origin": "^https?://(imglf(\\d)?.nosdn.127.net/img/\\w+.(jpg|jpeg|gif|png|bmp|webp))\\?.*",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame",
        "image"
      ],
      "target": "https://$1",
      "example": "https://imglf.nosdn.127.net/img/T0VLUWtpQlNHSmVHYlM0QU01cElqRE0vM09ETzRKTEtjK3hROFpqSk9QMm1GWXlaL2EwQ1hBPT0.jpeg?imageView&thumbnail=500x10000&quality=96&stripmeta=0&type=jpg%7Cwatermark&type=1&dx=0&gravity=southwest&dissolve=100&image=aW1nL3N5cy9zZWxlY3Rpb24tcnNzLXdhdGVybWFyay5wbmc=&ssitoken_1479551598431_",
      "enable": true
    },
    {
      "description": "原图显示：Tumblr；可作用于InoReader",
      "origin": "^https?://(\\d+).media.tumblr.com/(.+tumblr_\\w+_)500.(jpg|jpeg|gif|png|bmp|webp)(\\?ssitoken_\\d+_)?$",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame",
        "image"
      ],
      "target": "https://$1.media.tumblr.com/$21280.$3",
      "example": "https://68.media.tumblr.com/3beca4f8c1e028a29e87394de17c86b1/tumblr_o2qh8ztGAt1r7hrego2_500.jpg",
      "enable": true
    },
    {
      "description": "原图显示：Tumblr 失效的外链图床；测试见：https://www.reddit.com/domain/$1.media.tumblr.com/",
      "origin": "^https?://(41|65|67|78).media.tumblr.com/",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame",
        "image"
      ],
      "target": "https://68.media.tumblr.com/",
      "example": "http://41.media.tumblr.com/2e2b4f5c659dca9b09d9b8a881656d82/tumblr_o0y4zmaRxk1t52mpao1_500.jpg",
      "enable": true
    },
    {
      "description": "原图显示：TPRSS 生成的RSS图片直接显示原图（如果作用于 image，会导致 InoReader 破图，只能作用于地址栏。）",
      "origin": "^https?://tprss.puteulanus.com/index.php\\?pic=https?://pbs.twimg.com/media/(\\S+).(jpg|jpeg|gif|png|bmp|webp)(&ssitoken_\\d+_)?",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame"
      ],
      "target": "https://pbs.twimg.com/media/$1.$2:orig",
      "example": "http://tprss.puteulanus.com/index.php?pic=http://pbs.twimg.com/media/C6T3cdEU4AEcnf-.jpg",
      "enable": true
    },
    {
      "description": "原图显示：修复挖矿网 RSS 输出错误",
      "origin": "^https://wk588.com/wp-content/uploads/(\\d{4}/\\d{2}/\\S+.(jpg|jpeg|gif|png|bmp|webp))$",
      "exclude": "",
      "methods": [],
      "types": [
        "main_frame",
        "image"
      ],
      "target": "https://img.wk588.com/wp-content/uploads/$1",
      "example": "https://wk588.com/wp-content/uploads/2018/01/134011hyjgwm9qmukzumy2.png",
      "enable": true
    }
  ]
}
