{
  "version": "2",
  "createdAt": "2017-03-02",
  "rules": [
    {
      "description": "Google -> 科大 1",
      "origin": "(ajax|fonts).googleapis.com",
      "exclude": "",
      "methods": [],
      "types": [],
      "target": "$1.proxy.ustclug.org",
      "example": "https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js",
      "exampleURL": "https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js",
      "enable": true
    },
    {
      "description": "Google -> 科大 2",
      "origin": "themes.googleusercontent.com",
      "exclude": "",
      "methods": [],
      "types": [],
      "target": "google-themes.proxy.ustclug.org",
      "example": "https://themes.googleusercontent.com/static/fonts/antic/v4/hEa8XCNM7tXGzD0Uk0AipA.ttf",
      "enable": true
    },
    {
      "description": "谷歌字体疑似解封，故禁用该规则",
      "origin": "fonts.gstatic.com",
      "exclude": "",
      "methods": [],
      "types": [],
      "target": "fonts-gstatic.proxy.ustclug.org",
      "example": "https://fonts.gstatic.com/ea/cwtexhei/v3/cwTeXHei-zhonly.ttf",
      "enable": false
    },
    {
      "description": "Google -> rawgit",
      "origin": "apis.google.com/js/(api|plusone).js$",
      "exclude": "",
      "methods": [],
      "types": [],
      "target": "cdn.rawgit.com/jiacai2050/gooreplacer/gh-pages/proxy/$1.js",
      "example": "https://apis.google.com/js/api.js",
      "enable": true
    },
    {
      "description": "Twitter -> rawgit",
      "origin": "platform.twitter.com/widgets.js",
      "exclude": "",
      "methods": [],
      "types": [],
      "target": "cdn.rawgit.com/jiacai2050/gooreplacer/gh-pages/proxy/widgets.js",
      "example": "https://platform.twitter.com/widgets.js",
      "enable": true
    }
  ]
}
